{
  "name": "phototech/sailvenice",
  "description": "Sail Venice Website.",
  "type": "project",
  "license": "AGPL-3.0",
  "require": {
    "composer/installers": "^1.0.20",
    "drupal/core": "~8.0",
    "drupal/address": "~1.0",
    "drupal/field_group": "~1.0",
    "drupal/ds": "~2.0",
    "drupal/name": "~1.0",
    "drupal/contact_storage": "~1.0-beta2",
    "drupal/simple_gmap": "~1.0",
    "drupal/paragraphs": "~1.0",
    "sailvenice/patches": "~1.0",
    "drupal/stage_file_proxy": "~1.0",
    "drupal/contact_link": "~1.0-alpha1",
    "drupal/display_field_copy": "~1.0-alpha1",
    "drupal/purge": "~3.0-beta4",
    "drupal/purge_purger_http": "~1.0-beta3"
  },
  "require-dev": {
    "drush/drush": "~8.0",
    "drupal/coder": "~8.2.5",
    "mikey179/vfsStream": "~1.2",
    "phpunit/phpunit": "~4.8",
    "behat/mink": "~1.7",
    "drupal/console": "^0.10.13"
  },
  "replace": {
    "drupal/drupal": "~8.0"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "repositories": [
    {
      "type": "composer",
      "url": "https://packages.drupal.org/8"
    },
    {
      "type": "package",
      "package": {
        "type": "metapackage",
        "name": "sailvenice/patches",
        "version": "1.5.0",
        "require": {
          "netresearch/composer-patches-plugin": "^1.2.1"
        },
        "extra": {
          "patches": {
            "drupal/contact_storage": [
              {
                "title": "Issue #2629630: When no Redirect Page is specified; redirect to parent entity",
                "url": "https://www.drupal.org/files/issues/contact_storage-default_redirect-2629630-2.patch",
                "args": "-F 0"
              }
            ],
            "drupal/purge": [
              {
                "title": "Issue #2699897: Test method \"testHelper\" in test class \"Drupal\\Tests\\purge\\Unit\\Logger\\LoggerChannelPartTest\" is not public",
                "url": "https://www.drupal.org/files/issues/test_helper-2699897-2.patch"
              }
            ]
          }
        }
      }
    }
  ],
  "config": {
    "preferred-install": "dist",
    "autoloader-suffix": "Drupal8"
  },
  "extra": {
    "installer-paths": {
      "web/core": ["type:drupal-core"],
      "web/modules/contrib/{$name}": ["type:drupal-module"],
      "web/profiles/contrib/{$name}": ["type:drupal-profile"]
    }
  }
}
