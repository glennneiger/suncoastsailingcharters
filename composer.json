{
  "name": "phototech/sailvenice",
  "description": "Sail Venice Website.",
  "type": "project",
  "license": "AGPL-3.0",
  "require": {
    "composer/installers": "^1.0.20",
    "drupal/core": "~8.0",
    "drupal/address": "~1.0",
    "drupal/field_group": "~1.0",
    "drupal/ds": "~3.0",
    "drupal/name": "~1.0",
    "drupal/contact_storage": "~1.0-beta2",
    "drupal/simple_gmap": "~1.0",
    "drupal/paragraphs": "~1.0",
    "sailvenice/patches": "~3.0",
    "drupal/stage_file_proxy": "~1.0",
    "drupal/contact_link": "~1.0-alpha1",
    "drupal/display_field_copy": "~1.0-alpha1",
    "drupal/purge": "~3.0-beta4",
    "drupal/purge_purger_http": "~1.0-beta3",
    "drupal/imagemagick": "^1.0.0-alpha2",
    "drupal/token_filter": "^1.0.0-beta1",
    "drupal/advagg": "^2.3.0",
    "drupal/image_effects": "^1.0.0",
    "drupal/layout_plugin": "^1.0.0",
    "drupal/sailor": "^1.0",
    "drush/drush": "~8.0",
    "drupal/console": "~1.0"
  },
  "replace": {
    "drupal/drupal": "~8.0"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "repositories": [
    {
      "type": "composer",
      "url": "https://packages.drupal.org/8"
    },
    {
      "type": "package",
      "package": {
        "type": "metapackage",
        "name": "sailvenice/patches",
        "version": "3.0.0",
        "require": {
          "netresearch/composer-patches-plugin": "^1.2.1"
        },
        "extra": {
          "patches": {
            "drupal/core": [
              {
                "title": "Issue #2613926: The Pecl UUID implementation can return invalid uuids",
                "url": "https://www.drupal.org/files/issues/pecl-uuid-2613926-2.patch",
                "args": "--directory=../"
              }
            ]
          }
        }
      }
    }
  ],
  "config": {
    "preferred-install": "dist",
    "autoloader-suffix": "Drupal8"
  },
  "extra": {
    "installer-paths": {
      "html/core": ["type:drupal-core"],
      "html/modules/contrib/{$name}": ["type:drupal-module"],
      "html/themes/contrib/{$name}": ["type:drupal-theme"],
      "html/profiles/contrib/{$name}": ["type:drupal-profile"]
    }
  },
  "scripts": {
    "drupal": "drupal --root=html",
    "serve": "drupal --root=html server"
  }
}
