{
  "name": "phototech/sailvenice",
  "description": "Sail Venice Website.",
  "type": "project",
  "license": "AGPL-3.0",
  "require": {
    "composer/installers": "^1.0.20",
    "drupal/core": "~8.0",
    "drupal/address": "^8.1.0",
    "drupal/field_group": "^8.1.0",
    "drupal/ds": "^8.2.0",
    "drupal/name": "^8.1.0",
    "drupal/contact_storage": "^8.1.0-beta2",
    "drupal/simple_gmap": "^8.1.0",
    "drupal/paragraphs": "^8.1.0",
    "drupal/config_installer": "^8.1.2",
    "sailvenice/patches": "~1.0"
  },
  "require-dev": {
    "drush/drush": "~8.0",
    "drupal/coder": "~8.2.5",
    "mikey179/vfsStream": "~1.2",
    "phpunit/phpunit": "~4.8"
  },
  "replace": {
    "drupal/drupal": "~8.0"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "repositories": [
    {
      "type": "composer",
      "url": "http://packagist.drupal-composer.org"
    },
    {
      "type": "package",
      "package": {
        "type": "metapackage",
        "name": "sailvenice/patches",
        "version": "1.2.0",
        "require": {
          "netresearch/composer-patches-plugin": "~1.0"
        },
        "extra": {
          "patches": {
            "drupal/contact_storage": [
              {
                "title": "Issue #2629630: When no Redirect Page is specified; redirect to parent entity",
                "url": "https://www.drupal.org/files/issues/contact_storage-default_redirect-2629630-2.patch"
              }
            ],
            "drupal/ds": [
              {
                "title": "Issue #2633728: Multiple Custom Blocks on a page renderes the first block's content",
                "url": "https://www.drupal.org/files/issues/2633728-9.patch"
              }
            ]
          }
        }
      }
    }
  ],
  "config": {
    "preferred-install": "dist",
    "autoloader-suffix": "Drupal8"
  },
  "extra": {
    "installer-paths": {
      "web/core": ["type:drupal-core"],
      "web/modules/contrib/{$name}": ["type:drupal-module"],
      "web/profiles/contrib/{$name}": ["type:drupal-profile"]
    }
  }
}
